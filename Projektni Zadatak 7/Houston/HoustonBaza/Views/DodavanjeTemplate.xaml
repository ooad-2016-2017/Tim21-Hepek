        <TextBox x:Name="textBoxZaNepobrojaneTipove" BorderThickness="1" HorizontalAlignment="Left" Margin="157,121,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="193"/>
        <TextBox x:Name="textBox5" BorderThickness="0" HorizontalAlignment="Left" Margin="10,121,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="142"/>
        <TextBox x:Name="textBox6" BorderThickness="0" HorizontalAlignment="Left" Margin="10,158,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="142"/>
        <TextBox x:Name="textBox7" HorizontalAlignment="Left" Margin="157,158,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="193"/>

    </Grid>
</Page>
