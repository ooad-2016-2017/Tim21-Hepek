        <TextBox x:Name="textBox3"  BorderThickness="1" HorizontalAlignment="Left" Height="0" Margin="157,47,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="193"/>
        <Button x:Name="button" Content="Nazad" Click="Nazad_Click" HorizontalAlignment="Left" Margin="285,598,0,0" VerticalAlignment="Top"/>
